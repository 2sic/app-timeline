@inherits Custom.Hybrid.Razor12
@using ToSic.Razor.Blade;
@{
  // 1. Get Services for Css and Page
  var pageCss = GetService<Connect.Koi.ICss>();         // Service to get CSS information about the current Theme
  var page = GetService<ToSic.Sxc.Web.IPageService>();  // Service to set titles etc. on the page

  // 2. Include Bootstrap4 if the framework isn't known
  if (pageCss.IsUnknown) {
    page.Activate("Bootstrap4");
  }

  page.Activate("turnOn", "FontAwesome5"); // see https://r.2sxc.org/turnon

  var domAttribute = "app-timeline-" + CmsContext.Module.Id;
}

@* <turnOn turn-on='{"run": "window.appTimeline.init()", "data": { "domAttribute": "@domAttribute" }}'></turnOn> *@
<link rel="stylesheet" href="@App.Path/bs5/dist/bs5.min.css" data-enableoptimizations="true">
@* <script src="@App.Path/bs4/dist/scripts.min.js" data-enableoptimizations="false"></script> *@